{
  "author": "Taiiwo",
  "description": "Add a blog thing to your site!",
  "website": "https://github.com/Taiiwo/TaiiCMS",
  "depends": ["layout"],
  "conflicts": [],
  "datachests": [
      {
          "name": "posts",
          "public": false
      }
  ],
  "pages": {
    "/blog": {
      "file_path": "blog-page.html"
    },
    "/blog/post/:id": {
      "file_path": "post-page.html"
    },
    "/blog/publish": {
      "file_path": "publish-page.html"
    }
  }
}
